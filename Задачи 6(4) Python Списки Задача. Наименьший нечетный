#Sholin
from random import randrange
a = [randrange(1,23) for i in range(randrange(7,23))]
b = []
for x in range(len(a)):
    if(a[x] % 2 != 0):
        b.append(a[x])
print(*a)
print(*b)
if(len(b) == 0):
    print(0)
elif(len(b) > 0):
    print(min(b))
    
    
#Soroka
import random
a=[random.randint(-10,10) for i in range(5)]
print(*a)
b=[]
ch=0
for i in a:
    if(i%2!=0):
        b.append(i)
    else:
        i=i
if(len(b)!=0):
    ch=min(b)
    print(ch)
else:
    print(ch)


#Lysenko
a = [X for X in input().split(' ')]
d=[]
for i in range(len(a)-1):
    if int(a[i])%2!=0:
        d.append(a[i])
if len(d)==0:
    s=0
else:
    s=min(d[:])
print(s)

#Fomenko
a=1000
for i in  input().split():
    if(int(i) % 2 == 1 and int(i) < a and int(i) > 0):
        a=int(i)
print(a)


#Mikaelan
a = [int(j) for j in input().split()]
print(min([a[i] for i in range(len(a)) if a[i] % 2 == 1]))   # Наименьший нечетный

print(min([int(i) for i in input().split() if int(i) % 2 == 1]))   #Наименьший нечетный
