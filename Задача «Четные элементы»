s=input()
a=[int(s) for s in s.split()]
for i in a:
    if int(i)%2 == 0:
       print(i, end=' ')
       
       
a = input().split()
for i in range(len(a)):
    a[i] = int(a[i])
    if a[i] % 2 == 0:
        print(a[i])
        
        
#Mikaelan 
from random import randrange
a = [randrange(1, 30) for i in range(randrange(7, 10))]
print(*a)
b = [a[i] for i in range(len(a)) if a[i] % 2 == 0]  # Четные элементы
print(*b)


