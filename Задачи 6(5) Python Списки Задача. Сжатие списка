#Soroka
import random
a=[random.randint(0,1) for i in range(12)]
print(*a)
b=a.count(0)
c=len(a)-b
i=0
while(len(a)!=c):
    if(a[i]==0):
        del a[i]
    else:
        i+=1
for i in range(b):
    a.append(0)
print(*a)


#Rogala
a = [4,0,5,0,3,0,0,5]
for i in range(len(a)):   #сжатие списка
    if int(a[i]) == 0:
       a.append(a[i])
       a.remove(a[i])
print(a)

